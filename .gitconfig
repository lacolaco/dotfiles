[core]
	excludesfile = /Users/lacolaco/.gitignore_global
	autocrlf = input
	editor = code --wait
[user]
	email = suguru.inatomi@gmail.com
	name = Suguru Inatomi
	signingkey = ~/.ssh/id_ed25519.pub
[color]
	ui = auto
[url "git@github.com:"]
    # insteadOf = https://github.com/
[init]
	defaultBranch = main
[gpg]
	format = ssh
[commit]
	gpgsign = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
